<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
<servlet>
  <servlet-name>userServicesImpl</servlet-name>
  <servlet-class>com.atos.profilergwt.server.UserServicesImpl</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>userServicesImpl</servlet-name>
  <url-pattern>/profilergwt/user</url-pattern>
</servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>ProfilerGWT.html</welcome-file>
  </welcome-file-list>

</web-app>
