<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='profilerwebgwt'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.clean.Clean' />
	<inherits name="org.mvp4j" />
	<inherits name="org.mvp4j-impl" />
	<inherits name="com.gwtent.reflection.Reflection" />
	<inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" />
	<set-property name="log_ConsoleLogger" value="ENABLED" />
	<set-property name="log_DivLogger" value="ENABLED" />
	<set-property name="log_FirebugLogger" value="ENABLED" />
	<set-property name="log_GWTLogger" value="ENABLED" />
	<set-property name="log_SystemLogger" value="ENABLED" />
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->

	<!-- Specify the app entry point class. -->
	<entry-point class='com.profiler.client.ProfilerWebGWT' />
	<servlet path='/profilerwebgwt/user' class='com.profiler.server.UserServicesImpl' />
	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
	<source path='presenter' />
	<source path='model' />

</module>
